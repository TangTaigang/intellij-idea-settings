      <place><![CDATA[psiParameter().ofMethod(0, psiMethod().withName("println").withParameters("java.lang.String").definedInClass("java.io.PrintWriter"))]]></place>
    </injection>
    <injection language="JSON" injector-id="java">
      <display-name>WechatController.createOrder (com.dx.ucupay.wechat.controller)</display-name>
